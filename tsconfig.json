{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "lib": ["dom", "esnext"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "preserve",
    "module": "esnext",
    "isolatedModules": true
  },
"include": ["src/**/*.ts", "src/**/*.tsx", "src/challenges/*/src/**/*.ts", "src/challenges/*/src/**/*.tsx"]
}
