{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "lib": ["dom", "esnext"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "isolatedModules": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "001/src/**/*.ts", "001/src/**/*.tsx"]
}
